{
  "version": 2,
  "name": "studio-pro-website",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/api/uploads.js"
    },
    {
      "src": "/(admin|booking|payment-gateway|wedding-photography|services|auth|privacy-policy|terms-conditions|cookie-policy)",
      "dest": "/$1.html"
    },
    {
      "src": "/admin-dashboard",
      "dest": "/admin-dashboard.html"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "MONGODB_URI": "mongodb://localhost:27017/studiopro",
    "JWT_SECRET": "your_jwt_secret_here",
    "NODE_ENV": "production",
    "CONTACT_EMAIL": "dubeyatharv36@gmail.com",
    "CONTACT_PHONE": "7021763330",
    "BUSINESS_LOCATION": "Mumbai, Maharashtra, India"
  }
}